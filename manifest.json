{
  "manifest_version": 3,
  "name": "Random Meme Generator",
  "version": "1.0",
  "description": "Generate random memes with the click of a button!",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
